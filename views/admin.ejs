<h1 class="mt-4">Admin Dashboard</h1>
<p class="lead mb-3">Welcome
    <%= user.name %>
</p>

<h2>Current sessions</h2>

<% for (key in sessions){ %>
    <p>Session ID:
        <%= key %><br> User Id:
            <%= JSON.parse(sessions[key]).passport.user %> <br>
                <form action="/destroy/:<%= key %>" method="POST">
                    <button type="submit" class="btn btn-primary btn-block">Revoke session</button>
                </form>
    </p>
    <% } %>
        <a href="/auth/logout" class="btn btn-secondary">Logout</a>
